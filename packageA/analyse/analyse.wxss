page {
  height: 100%;
}

.container {
  height: 100%;
  background-color: #000;
  position: relative;
}

.analysing {
  width: 602rpx;
  height: 602rpx;
  background-color: #fff;
  position: absolute;
  left: 50%;
  top: 46%;
  transform: translate(-50%, -50%);
  text-align: center;
}

.analysing .img, .faild .img, .success .img {
  width: 198rpx;
  height: 220rpx;
  margin: 80rpx 0;
}

.analysing .info1 {
  font-size: 34rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(51, 51, 51, 1);
  line-height: 48rpx;
}

.analysing .info2 {
  font-size: 24rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(153, 153, 153, 1);
  line-height: 34rpx;
  margin-top: 20rpx;
}

.faild {
  width: 602rpx;
  height: 740rpx;
  background-color: #fff;
  position: absolute;
  left: 50%;
  top: 46%;
  transform: translate(-50%, -50%);
  text-align: center;
}

.faild .bottom {
  border-top: 1px #ececec dashed;
  box-sizing: border-box;
  padding-top: 30rpx;
  margin-top: 40rpx;
}

.faild .title {
  font-size: 24rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(51, 51, 51, 1);
  line-height: 34rpx;
}

.faild .reason {
  font-size: 24rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(153, 153, 153, 1);
  line-height: 34rpx;
  margin-top: 20rpx;
}

.faild .reupload {
  position: absolute;
  bottom: -140rpx;
  width: 200rpx;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 4px;
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(255, 255, 255, 1);
  background-color: #000;
  border: 1px solid #fff;
  left: 50%;
  transform: translateX(-50%);
  z-index: 12;
}

.success {
  height: 100%;
  width: 100%;
  background-color: #fff;
  text-align: center;
}

.success .info1 {
  font-size: 34rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(51, 51, 51, 1);
  line-height: 48rpx;
}

.success .info2 {
  font-size: 24rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(255, 123, 33, 1);
  line-height: 34rpx;
  margin-top: 20rpx;
}

.success .bottom {
  text-align: left;
  border-top: 1px #ececec dashed;
  margin-top: 40rpx;
  padding: 80rpx 40rpx;
}

.success .tit {
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(51, 51, 51, 1);
  line-height: 40rpx;
}

.success .add {
  width: 23px;
  height: 14px;
  border-radius: 6px;
}

.success .iconlist {
  display: flex;
  flex-direction: row;
  flex-flow: wrap;
  margin-top: 15px;
}

.success .icon-item {
  padding: 0 24rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 24rpx;
  font-family: PingFang-SC-Regular;
  color: rgba(253, 143, 47, 1);
  border: 1px #FD8F2F solid;
  border-radius: 13px; 
  margin: 15rpx 25rpx;
  max-width: 180rpx;
  box-sizing: border-box;
  overflow: hidden;
  /* white-space: nowrap;
  text-overflow: ellipsis; */
}

.success .back {
  border: 1px #ececec solid;
  width: 100px;
  height: 70rpx;
  border-radius: 4px;
  font-size: 15px;
  font-family: PingFang-SC-Medium;
  color: rgba(51, 51, 51, 1);
  line-height: 70rpx;
  text-align: center;
  position: absolute;
  bottom: 60rpx;
  left: 50%;
  transform: translateX(-50%);
}

.add-icon-modal{
  width: 502rpx;
  /* height: 200rpx; */
  background-color: rgba(253, 143, 47, 1);
  position: absolute;
  left: 50%;
  top: 46%;
  transform: translate(-50%, -50%);
  padding: 40rpx;
}

.add-icon-input{
  background-color: #fff;
  font-size: 28rpx;
  padding-left: 30rpx;
}
