.cutoffTitle-container{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  text-align: center;
  font-size: 30rpx;
  position: relative;
  padding: 40rpx;
  box-sizing: border-box;
}

.title{
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  padding: 0 40rpx;
  font-weight: 700;
}

.line{
  height: 2rpx;
  width: 100%;
  background-color: #ececec;
}

.titleBg{
  background-color: #000;
}